<form class="new_job_application" id="new_job_application" action="/job_applications" accept-charset="UTF-8" method="post">
  <p>
    <%= f.label :full_name %>
    <%= f.text_field :full_name %>
  </p>

  <p>
    <%= f.label :phone %>
    <%= f.text_field :phone %>
  </p>

  <p>
    <%= f.label :hobby %>
    <%= f.text_field :hobby %>

  <p>
    <%= f.label :email %>
    <%= f.text_field :email%>

  <p>
    <%= f.label :available_date %>
    <%= f.text_field :available_date %>
  </p>

  <p>
    <input type="submit" name="commit" value="Create Job application" />
  </p>
</form>
